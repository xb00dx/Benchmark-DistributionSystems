//*********************************************
// Transformer config Objects
//*********************************************

object transformer_configuration {
	name Xfrm_config_PV_300014117_A;
	connect_type SINGLE_PHASE_CENTER_TAPPED;
	install_type POLETOP;
	power_rating 500000;
	primary_voltage 7200;
	secondary_voltage 120;
	resistance 0.09;
	reactance 1.81;
}

object transformer_configuration {
	name Xfrm_config_PV_300014117_B;
	connect_type SINGLE_PHASE_CENTER_TAPPED;
	install_type POLETOP;
	power_rating 500000;
	primary_voltage 7200;
	secondary_voltage 120;
	resistance 0.09;
	reactance 1.81;
}

object transformer_configuration {
	name Xfrm_config_PV_300014117_C;
	connect_type SINGLE_PHASE_CENTER_TAPPED;

//*********************************************
// Transformer Objects
//*********************************************

object transformer {
	name Xfrm_300014117_A;
	phases AS;
	from N_300014117;
	to N_300014117_sec_A;
	configuration Xfrm_config_PV_300014117_A;
}

object transformer {
	name Xfrm_300014117_B;
	phases BS;
	from N_300014117;
	to N_300014117_sec_B;
	configuration Xfrm_config_PV_300014117_B;
}

object transformer {
	name Xfrm_300014117_C;
	phases CS;
	from N_300014117;
	to N_300014117_sec_C;
	configuration Xfrm_config_PV_300014117_C;
}

//*********************************************
// Triplex meter Objects
//*********************************************

object triplex_meter {
	name N_300014117_sec_A;
	phases AS;
	nominal_voltage 120;
}

object triplex_meter {
	name N_300014117_sec_B;
	phases BS;
	nominal_voltage 120;
}

object triplex_meter {
	name N_300014117_sec_C;
	phases CS;
	nominal_voltage 120;
}


//*********************************************
// Inverter Objects
//*********************************************

object inverter {
	name inverter_300014117_A;
	parent N_300014117_sec_A;
	phases AS;
	generator_mode CONSTANT_PF;
	generator_status ONLINE;
	inverter_type PWM;
	power_factor 1.000000;
	rated_power 500000.0;
	inverter_efficiency 0.9;
}

object inverter {
	name inverter_300014117_B;
	parent N_300014117_sec_B;
	phases BS;
	generator_mode CONSTANT_PF;
	generator_status ONLINE;
